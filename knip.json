{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}"
  ],
  "project": [
    "src/app/**/*.{ts,tsx}",
    "src/page/**/*.{ts,tsx}",
    "src/api/**/*.{ts,tsx}",
    "src/state/**/*.{ts,tsx}"
  ],
  "ignoreExportsUsedInFile": true,
  "include": [
    "files"
  ],
  "exclude": [
    "dependencies",
    "devDependencies",
    "optionalPeerDependencies",
    "unlisted",
    "binaries",
    "exports",
    "types",
    "duplicates"
  ]
}
